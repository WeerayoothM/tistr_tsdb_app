{
  "expo": {
    "name": "R Plus",
    "sdkVersion": "48.0.0",
    "slug": "rplus",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "rplus",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tistr.rplus"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-android.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.tistr.rplus",
      "versionCode": 3
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "packagerOpts": {
      "sourceExts": ["js", "jsx", "svg", "ts", "tsx"]
    },
    "extra": {
      "eas": {
        "projectId": "c1065f50-a563-4ccd-92dd-dfff67717fa0"
      }
    }
  }
}
